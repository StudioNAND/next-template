{
  "name": "electron-next",
  "version": "0.0.1",
  "main": "main/index.js",
  "license": "MIT",
  "repository": "StudioNAND/electron-next",
  "scripts": {
    "start": "node ./server/index.js",
    "build": "next build",
    "serve": "NODE_ENV=production node ./server/index.js",
    "precommit-msg": "echo '\n Running prettier and eslint before commiting \n'",
    "prettier": "prettier --config ./node_modules/@studionand/eslint-config/prettier.config.js --ignore-path .eslintignore --list-different \"src/**\" \"components/**\" \"server/**\" \"pages/**\" \"utils/**\" \"stores/**\"",
    "prettier:fix": "prettier --config ./node_modules/@studionand/eslint-config/prettier.config.js --ignore-path .eslintignore --write \"src/**\" \"components/**\" \"server/**\" \"pages/**\" \"utils/**\" \"stores/**\"",
    "eslint": "eslint stores server utils pages components",
    "eslint:fix": "eslint --fix stores server utils pages components"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@studionand/eslint-config": "0.0.22",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "webpack": "^4.20.2"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "electron-is-dev": "^1.0.1",
    "electron-next": "^3.1.5",
    "express": "^4.16.4",
    "jss": "^9.8.7",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "next": "^7.0.2",
    "next-routes": "^1.4.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-jss": "^8.6.1",
    "styled-jsx": "^3.1.0"
  },
  "pre-commit": [
    "precommit-msg",
    "eslint"
  ],
  "eslintConfig": {
    "extends": [
      "@studionand/eslint-config"
    ],
    "rules": {}
  }
}
